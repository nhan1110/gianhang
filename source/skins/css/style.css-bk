/*=======================#top_bar=======================*/
#top_bar{
    position: relative;
    margin-bottom: 0;
    margin-top: 10px;
}
#top_bar .logo{
    position: absolute;
    top: 0;
    z-index: 100;
}
.cart{
    position: absolute;
    top: -10px;
    right: 0;
    z-index: 101;
}
.cart .navbar-nav>li:after{
    content: '';
    position: absolute;
    width: 1px;
    height: 23px;
    border-right: 1px solid #ccc;
    top: 32%;
    right: 0;
}
.cart .navbar-nav>li:last-child:after{
    display: none;
}
.cart .navbar-nav>li>a{
    background: #fff;
    color: #999999;
    font-size: 12px;
    line-height: 12px;
    padding-top: 10px;
    padding-bottom: 10px;
    transition:all .5s ease-in-out;
    -webkit-transition:all .5s ease-in-out;
    -moz-transition:all .5s ease-in-out;
}
.cart .navbar-nav > li.open > a,
.cart .navbar-nav > li > a:hover {
    color: #ff7436;
}
.cart .navbar-nav>li>a>span.fa-user{
    font-size: 23px;
}
.cart .navbar-nav>li>a>span{
    font-size: 20px;
}
.cart .navbar-nav>li>a>i{
    vertical-align: super;
    background-color: red;
    color: #ffffff;
    font-style: normal;
}
.cart .navbar-nav>li>a:hover,
.cart .navbar-nav>li>a:focus{
    background-color: transparent;
}
#top_bar .navbar-form{
    position: relative;
    top: -10px;
    margin-bottom: 0;
}
#top_bar .navbar-form .form-group{

}
#top_bar .navbar-form .form-group select.form-control{
    margin-left: -5px;
