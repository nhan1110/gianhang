/*Header Mobile*/
@media (max-width: 768px) {
    #header #top_bar{
        position: fixed;
        margin-top: 0;
        top: 0;
        bottom: 0;
        left: -100%;
        width: 100%;
        z-index: 100;
        background: #fff;
        overflow-y:auto;
        /*visibility: hidden;*/
        transition-property: right, left;
        transition-duration: 0.3s;
        -webkit-transition-property: right, left;
        -webkit-transition-duration: .3s;
    }
    #header #top_bar.open{
        left: 0;
        /*visibility: visible;*/
    }
    #header #top_bar .close-mobile-menu{
        margin-bottom: 0;
        position: absolute;
        right: 25px;
        top: 5px;
    }
    #header #top_bar .logo,
    #header #top_bar .navbar-form{
        display: none !important;
    }

    #header #top_bar .menu-mobile-category ul{
        list-style: none;
        padding-left: 20px;
        padding-top: 10px;
    }
    #header #top_bar .menu-mobile-category ul li{
        position: relative;
        margin-bottom: 10px;
        background: none !important;
    }
    #header #top_bar .menu-mobile-category ul li a{
        display: block;
        padding: 3px 0;
        clear: both;
        font-weight: 400;
        line-height: 1.42857143;
        color: #333;
        white-space: nowrap;
        transition: all .5s ease-in-out;
        -webkit-transition: all .5s ease-in-out;
        -moz-transition: all .5s ease-in-out;
        background: none !important;
    }
    #header #top_bar .menu-mobile-category ul li img {
        position: absolute;
        right: 5px;
        top: 0;
        z-index: 100;
        width: 25px;
        height: 25px;
    }
    #header .header-mobile{
        padding:10px 0;
    }
    #nav #catalog_menu{
        display: none !important;
    }
    #nav a.bar-add-product{
        margin-left: -15px;
        margin-right: -15px;
        margin-top: -1px;
    }
}