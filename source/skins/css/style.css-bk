    border-top: 1px solid #E6E6E6;
    border-bottom: 1px solid #E6E6E6;
    border-left: 4px solid #F3984D;
    font-size: 16px;
    padding: 10px 15px;
    margin-top: 0;
    margin-bottom: 0;
}
#page-search .filter-left .base{
    margin-bottom: 20px;
}
