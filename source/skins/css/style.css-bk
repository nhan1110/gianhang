/*=======================.step_cart=======================*/
.step_cart{
    display: table;
    table-layout: fixed;
    width: 900px;
    color: #aaaaaa;
    position: relative;
    top: -80px;
}
.step_cart>li{
    position: relative;
    display: table-cell;
    text-align: center;
}
.step_cart>li.active:before{
    color: #FFF;
    background: #71be0f;
}
.step_cart>li.active>a{
    color: #71be0f;
}
.step_cart>li>a{color: #aaaaaa}
.step_cart>li:before{
    color: #fff;
    content: attr(data-step);
    display: block;
    margin: 0 auto 15px;
    background: #dddddd;
    width: 22px;
    height: 22px;
    text-align: center;
    line-height: 22px;
    border-radius: 100%;
    position: relative;
    z-index: 910;
}
.step_cart>li:after {
    content: '';
    position: absolute;
    display: block;
    background: #dddddd;
    width: 90%;
    height: 7px;
    top: 8px;
    left: 55%;
    z-index: 20;
}
.step_cart>li:last-child:after{display: none}
@media (max-width: 1200px){
    .step_cart{
        width: 600px;
    }
}
@media (max-width: 992px){
    .step_cart{
        position: static;
        width: 100%;
        margin-top: 20px;
    }
}
@media (max-width: 560px){
    .step_cart>li>a,
    .step_cart>li>span{display: none}
}
/*=======================.table_list=======================*/
.table_list{
    width: 100%;
    background-color: #FFFFFF;
}
.table_list tr>th,
.table_list tr>td{
    padding: 15px 20px;
}
.table_list tr>td{
    vertical-align: top;
    border-top: 1px solid #dddddd;
}
.table_list tr>td.image{
    width: 1%;
    padding-right: 0;
}
.table_list tr>td.image img{
    max-width: 60px;
}
.table_list tr>th{
    border-bottom: 1px solid #d2d2d2;
    color: #777777;
    font-size: 13px;
    font-weight: 400;
}
@media (max-width: 767px){
    .table_list thead tr>th{  display: none;  padding: 8px 10px;  }
    .table_list thead tr>th.count{display: block}
    .table_list tbody tr{
        display: block;
        position: relative;
        padding: 10px 0 10px 50px;
        border-bottom: 1px solid #dddddd;
    }
    .table_list tbody tr:last-child{
        border: none;
    }
    .table_list tbody tr td{
        display: block;
        padding: 0 10px;
        border-top: 0;
    }
    .table_list tbody tr td.image{
        position: absolute;
        top: 4px;
        left: 0;
        padding: 10px 10px;
        width: 60px;
    }
    .table_list tbody tr td.image img{
        max-width: 40px;
        max-height: 40px;
    }
    .table_list tbody tr td.name{
        padding-right: 50px;
    }
    .table_list tbody tr td.action{
        position: absolute;
        padding: 0;
        top: 10px;
        right: 10px;
    }
    .table_list tbody tr td.action .btn{padding: 0}
    .table_list tbody tr td.total{display: none}
    .table_list tbody tr td.quantity .form-control{
        width: inherit;
    }
    .table_list tbody tr td.unit{color: red}
}
