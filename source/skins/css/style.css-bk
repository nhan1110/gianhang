}
#page-search .filter-left .base .list-check li{
    padding-left: 20px;
}
.btn-show-filter{
    margin-bottom: 20px;
}
.show-mobile{
    display: none;
}
.sale{
    position: absolute;
    right: 0;
    top: 0;
}
@media (max-width: 768px) {
    #page-search .content-search{
        padding-left: 0;
        padding-right: 0;
    }
    .show-mobile{
        display: block;
    }
    #page-search .filter-left{
        position: absolute;
        top: 20px;
        bottom: 0;
        left: 0;
        width: 100%;
        z-index: 100;
        overflow-y: auto;
