    width: 85%;
    margin: 0 auto;
    margin-top: 15px;
}
#page-search .filter-left .base .nav li{
    position: relative;
}
#page-search .filter-left .base .list-custom li:before{
    content: "\f105";
    position: absolute;
    display: inline-block;
    font: normal normal normal 14px/1 FontAwesome;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    font-size: 22px;
    left: 20px;
    top: 5px;
    z-index: 100;
}
#page-search .filter-left .base .list-custom li a{
