    left: -5px;
}
.product .thumbnail .caption .product_price .price_primary .discount:before{
    content: '';
    position: absolute;
    top: calc((100% - 8px)/2);
    left: -7px;
    width: 0;
    height: 0;
    border-top: 4px solid transparent;
    border-bottom: 4px solid transparent;
    border-right: 7px solid #ed1c24;
}
.product .thumbnail .caption .product_price .price_primary .discount{
    position: relative;
    top: -4px;
    color: #ffffff;
