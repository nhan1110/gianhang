.checkbox input[type=checkbox] { 
    display:none; 
}

.checkbox input[type=checkbox] + label:before {
    display: inline-block;
    content: "";
    width: 18px;
    height: 18px;
    background: #fff;
    border: 1px solid #ccc;
    margin-right: 7px;
}

.checkbox input[type=checkbox] + label span{
    position: relative;
    top: -4px;
    display: inline;
    font-weight: 400;
}

.checkbox input[type=checkbox]:checked + label:before { 
    content: "";
    background: url("../admin/dist/css/blue.png") no-repeat;
    background-position: -40px 0;
    border: none;
}

/*Custom Radio Button*/
.checkbox input[type=radio] {
    display: none;
}

.checkbox input[type=radio] + label:before {
    display: inline-block;
    content:"";
    cursor: pointer;
    width: 18px;
    height: 18px;
    border: 1px solid #ccc;
    background: #fff;
    border-radius: 100%;
    margin-right: 7px;
}
.checkbox input[type=radio] + label span{
    position: relative;
    top: -4px;
    display: inline;
    font-weight: 400;
}
.checkbox input[type=radio]:checked + label:before {
    content:"";
    background: url("../admin/dist/css/blue.png") no-repeat;
    background-position: -140px 0;
    border: none;
}

/*Page Search*/
#page-search{
