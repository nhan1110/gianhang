.container{
    max-width: 1260px;
    width: auto;
    padding-left: 10px;
    padding-right: 10px;
}
.wrapp{
    max-width: 100%;
    overflow: hidden;
}
.line-through{text-decoration: line-through;}
.row{margin: 0 -10px}
.row .row{
    margin: 0;
}
.checkbox label.warning ,.radio label.warning{
   border: none !important;
}
.checkbox label.warning:before{
    border: 1px solid red !important;
}
fieldset {
    padding: auto; 
    margin: auto;
    border: auto;
    min-width: auto; 
    border: 2px solid #c0c0c0;
    padding: 0.35em 0.625em 0.75em;
    text-align: left;
    margin-bottom: 30px;
}
fieldset legend{
    border:none;
    width: auto; 
    font-size: 16px;
    color: #2a2f34;
}
/*Custom Css*/
.panel-home .panel-body{
    padding-right: 0;
    padding-left: 0;
}
.thumbnail{
    text-decoration: none;
}
.thumbnail:hover{
    border-color: #337ab7;
}
.thumbnail .thumb-product{
    background-size: cover;
    background-repeat: no-repeat;
    width: 100%;
    height: 200px;
}
.thumbnail h3{
    margin-top: 5px;
}
.thumbnail h3 a{
    text-decoration: none;
    color: #555;
}
.thumbnail span.price {
    display: block;
    background: #F3984D;
    position: absolute;
    min-width: 20px;
    padding: 8px;
    color: white;
    font-size: 16px;
    opacity: 21;
    -webkit-border-bottom-right-radius: 4px;
    -moz-border-radius-bottomright: 4px;
    border-bottom-right-radius: 4px;
}
.thumbnail .caption p{
    margin-bottom: 0;
}
.thumbnail hr{
    margin-bottom: 10px;
    margin-top: 10px;
}
.thumbnail .tracking p > span{
    margin-right: 20px;
    font-size: 16px;
}
.thumbnail .tracking span:last-child{
    margin-right: 0;
}
a.bar-add-product{
    display: block;
    padding: 12px 20px;
    background-color: #feaf02;
    color: #fff !important;
    text-decoration: none !important;
    font-size: 14px;
    text-transform: uppercase;
}
a.bar-add-product img{
    margin-right: 5px;
}
.dropdown-after:after {
    content: "";
    width: 0;
    height: 0;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    border-bottom: 10px solid #ccc;
    position: absolute;
    top: -10px;
    left: 50%;
}
.header-login + .dropdown-after:after{
    left: 74%;
}
#header .dropdown-login{
    left: -200px;
    min-width: 320px;
}
#header .login-box-body,
#header .signup-box-body{
    padding: 20px;
}
#header .dropdown-signup{
    min-width: 320px;
    left: -135px;
}
.btn-facebook {
    color: #fff !important;
    background-color: #3b5998;
    border-color: rgba(0,0,0,0.2);
}
.btn-social>:first-child {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    width: 32px;
    line-height: 34px;
    font-size: 16px;
    text-align: center;
    border-right: 1px solid rgba(0,0,0,0.2);
}
.btn-google {
    color: #fff !important;
    background-color: #dd4b39;
    border-color: rgba(0,0,0,0.2);
}
.btn-social {
    position: relative;
    padding-left: 44px;
    text-align: left;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    border-radius: 0;
}

#catalog_menu .dropdown-menu li:first-child{
    padding-top: 10px;
}
#catalog_menu .dropdown-menu li:first-child img{
    top: 10px;
}
#catalog_menu .dropdown-menu li{
    position: relative;
    margin-bottom: 10px;
    background: none !important;
}
#catalog_menu .dropdown-menu li a{
    transition:all .5s ease-in-out;
    -webkit-transition:all .5s ease-in-out;
    -moz-transition:all .5s ease-in-out;
    background: none !important;
}
#catalog_menu .dropdown-menu li:hover a{
    color: #feaf02;
}
#catalog_menu .dropdown-menu li img{
    position: absolute;
    right: 5px;
    top:0;
    z-index: 100;
    width: 25px;
    height: 25px;
}
#catalog_menu .dropdown-menu {
    min-width: 260px;
}
@media (min-width: 768px) {
    .thumbnails.custom-product .col-sm-4{
        padding-right: 10px;
        padding-left: 10px;
    }
    .col-sm-20{
        width: 20%;
    }
    .col-sm-80{
        width: 80%;
    }
}
@media (min-width: 500px) and (max-width: 768px) {
    .col-mxs-6{
        width: 50%;
    }
}
@media (max-width: 768px ) {
    #nav .navbar-nav{
        margin-bottom: 0;
        margin-top: 0;
    }
    #catalog_menu > li.main > a {
        padding-top: 15px;
        padding-bottom: 15px;
    }
    #catalog_menu .dropdown-menu{
        position: absolute;
        z-index: 100;
        background: #fff;
        top: 100%;
        left: 0;
        float: left;
    }
    #header .dropdown-login,
    #header .dropdown-signup{
        min-width: 300px;
        position: absolute;
        z-index: 102;
        background: #fff;
        border: 1px solid rgba(0,0,0,.15);
        border-radius: 4px;
        -webkit-box-shadow: 0 6px 12px rgba(0,0,0,.175);
        box-shadow: 0 6px 12px rgba(0,0,0,.175);
    }
    #header .dropdown-login{
    	left: -85px;
    }
    .header-login + .dropdown-after:after {
	    left: 40% !important;
	}
	#header .dropdown-signup{
		left: -10px;
	}
    #header .dropdown-signup:after{
        left: 10% !important;
    }
    #top_bar .navbar-nav{
        margin-top: 0;
        margin-bottom: 0;
    }
    #top_bar .nav > li {
        display: inline-block !important; 
    }
    #top_bar .cart{
        position: initial; 
        text-align: left;
        padding-left: 20px;
        top: auto;
        right: auto;
    }
    #top_bar .logo {
        position: initial !important;
        text-align: center;
        top:auto;
        padding: 10px 0;
    }
    #top_bar .navbar-form .form-group{
        display: inline-block;
    }
    #top_bar .navbar-form  .group-input{
        width:40%;
    }
    #top_bar .navbar-form  .group-select{
        width:40%;
    }
    #top_bar .navbar-form .form-group input,
    #top_bar .navbar-form .form-group select{
        max-width: 100%;
        width: 100% !important;
    }
    #top_bar .navbar-form .btn-danger{
        margin-top: -2px;
    }
}
/*=======================#header=======================*/
#header .top{
    background-color: #f4f4f4;
    border-radius: 0;
    border: none;
    margin-bottom: 0;
    min-height: 36px;
}
#header .top .navbar-nav>li>a{
    padding-top: 8px;
